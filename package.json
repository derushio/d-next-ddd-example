  "private": true,
  "scripts": {
    "dev": "pnpm run-p next:dev db:generate:watch db:studio",
    "build": "pnpm run-s db:generate next:build",
    "start": "pnpm next:start",
    "lint": "pnpm next:lint",
    "format": "pnpm run-p format:prettier format:markdown",
    "format:prettier": "pnpm prettier -w package.json src",
    "format:markdown": "pnpm markdownlint --fix '**/*.md'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist coverage",
    "test": "pnpm run-s mermaid:validate-all test:unit test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cross-env CI=true playwright test --workers=4",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "playwright test --trace on",
    "test:e2e:headed": "playwright test --headed",
    "next:dev": "next dev --turbopack",
    "next:build": "next build",
    "next:build:turbo": "next build --turbopack",
    "next:start": "next start",
    "next:lint": "next lint",
    "db:generate": "pnpm prisma generate --sql",
    "db:generate:watch": "pnpm prisma generate --sql --watch",
    "db:format": "pnpm prisma format",
    "db:push": "pnpm prisma db push",
    "db:migrate:dev": "pnpm prisma migrate dev",
    "db:migrate:deploy": "pnpm prisma migrate deploy",
    "db:seed": "pnpm prisma db seed",
    "db:studio": "pnpm prisma studio --browser none",
    "hash:generate": "pnpm dotenvx run -- pnpm tsx ./src/tools/generateHash.ts",
    "mermaid:validate": "pnpm tsx scripts/validate-mermaid.ts",
    "mermaid:validate-all": "pnpm tsx scripts/validate-mermaid.ts --all",
    "mermaid:check-patterns": "pnpm tsx scripts/check-mermaid-patterns.ts",
    "ui:add": "npx shadcn@latest add",
    "ui:update": "npx shadcn@latest diff",
    "ui:list": "npx shadcn@latest list"
  },
  "prisma": {
    "seed": "pnpm tsx ./src/layers/infrastructure/persistence/prisma/seeds/seedTestUser.ts"
  }
}
